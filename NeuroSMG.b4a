		End If
		If File.Exists(File.DirInternal & "data", "") = False Then
			File.MakeDir(File.DirInternal, "data")
		End If

		ActiveView = "login"
		LoginInit
		
		'''''''''''''''''''''''' test only
		name = "test" : age = 999 : sex = "aeldari" : dhand = "both"
