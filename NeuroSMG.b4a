	ACToolBarLight1.SubTitle = "Операции с результатами"
	MainPanel.LoadLayout("archive")
	
	Dim list As List
	list = File.ListFiles(File.DirInternal & "/data")
	For i = 0 To list.Size - 1
		ArchiveCustomListView.AddTextItem(list.Get(i), 0)
	Next
