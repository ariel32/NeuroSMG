Sub ArchiveButtonSave_Click
	Dim arc As Archiver
	Log(File.ListFiles(File.DirInternal & "/data"))
	If File.ListFiles(File.DirInternal & "/data").Size > 0 Then
		arc.ASyncZipFolder(File.DirInternal & "/data", File.DirInternal & "/out", "archive.zip", "arc")
	Else
		Msgbox("А что архивировать собрались, если результатов нет?","Ошибка!")
	End If
End Sub

Sub arc_ZipDone(CompletedWithoutError As Boolean, NbOfFiles As Int)
	Dim FileToSend As String = "archive." & DateTime.Now & ".zip"
	File.Copy(File.DirInternal & "/out", "archive.zip", Starter.Provider.SharedFolder, FileToSend)
	
	Dim in As Intent
	in.Initialize(in.ACTION_SEND, "")
	in.SetType("text/plain")
	in.PutExtra("android.intent.extra.STREAM", Starter.Provider.GetFileUri(FileToSend))
	in.Flags = 1 'FLAG_GRANT_READ_URI_PERMISSION
	StartActivity(in)
End Sub













